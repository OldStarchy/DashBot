{
  "name": "dashbot",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Nicholas Sorokin <nicholas.sorokin@hotmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc -p tsconfig.prod.json",
    "watch": "tsc -w",
    "run": "node dist/main.js",
    "publish": "docker build -t oldstarchy/dashbot . && docker push oldstarchy/dashbot",
    "lint": "eslint src/**/*.ts",
    "test": "mocha \"test/**/*.ts\"",
    "test:debug": "mocha --inspect-brk \"test/**/*.ts\"",
    "test:watch": "mocha -w \"test/**/*.ts\"",
    "coverage": "nyc mocha --config .mocharc.js \"test/**/*.ts\""
  },
  "dependencies": {
    "@types/node": "^13.5.0",
    "discord.js": "^11.5.1",
    "haiku-random": "^1.0.0",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2"
  }
}
