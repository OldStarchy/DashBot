	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Tests",
			"port": 9229,
			"request": "launch",
			"runtimeArgs": ["test"],
			"runtimeExecutable": "yarn",
			"skipFiles": ["<node_internals>/**"],
			"type": "pwa-node"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug DashBot",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\dist\\main.js",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"args": ["--storage", "..\\storage"],
			"cwd": "${workspaceFolder}\\dist"
		}
	]
}
